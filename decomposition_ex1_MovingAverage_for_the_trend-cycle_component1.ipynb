   "source": [
    "library(forecast)\n",
    "\n",
    "ma.ts <- ts(c(20,10,30,35,10,60,50,40,90,65,55,120), frequency = 1)\n",
    "\n",
    "ma.ts\n",
    "\n",
    "ma(ma.ts, order = 3)\n",
    "\n",
    "ma(ma.ts, order = 4, centre = FALSE)\n",
    "\n",
    "ma(ma.ts, order = 4, centre = TRUE)\n",
    "\n",
    "#SMA(ma.ts, n = 3) 평활법때 배운 이동평균"
   ]
